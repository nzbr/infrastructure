{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:base",
  ],
  regexManagers: [
    {
      fileMatch: ["^.*\\.(j|lib)sonnet$"],
      matchStrings: [
        'image *: *"(?<depName>[^"]+):(?<currentValue>[^"]+)(@(?<currentDigest>sha256:[a-f0-9]+))?"',
      ],
      datasourceTemplate: "docker",
    },
  ]
}
